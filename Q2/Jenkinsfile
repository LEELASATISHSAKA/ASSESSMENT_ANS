pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                script {
                    // here you have to provide git checkout pipeline syntax generated by jenkins //
                }
            }
        }

        stage('Build') {
            steps {
                script {
                    sh '''  mvn validate
                            mvn compile
                            mvn test
                            mvn package '''
                }
            }
        }
    }
}

